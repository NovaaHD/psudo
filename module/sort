a = [5,3,4,6,8,2]
function bubble_sort(arr)
    for i = 1 to len(arr)
        for j = 0 to len(arr) - i
            print(arr[j])
            print(arr[j+1])
            if arr[j] < arr[j+1]
                break
            else
                tmp = arr[j]
                arr[j] = arr[j+1]
                arr[j+1] = tmp
bubble_sort(a)
